<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditProfile">

    <ImageView
        android:id="@+id/Profile_Edit_1"
        android:layout_width="95dp"
        android:layout_height="90dp"
        android:layout_marginStart="163dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="725dp"
        android:src="@drawable/profile_icon"
        android:translationX="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/Cancel_button"
        android:layout_width="77dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="318dp"
        android:layout_marginBottom="847dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Cancel"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,RtlCompat,TextSizeCheck" />

    <TextView
        android:id="@+id/Save_button"
        android:layout_width="77dp"
        android:layout_height="28dp"
        android:layout_marginStart="318dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="847dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Save"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,RtlCompat,TextSizeCheck" />

    <TextView
        android:id="@+id/Edit_Profile_Picture"
        android:layout_width="42dp"
        android:layout_height="27dp"
        android:layout_marginStart="193dp"
        android:layout_marginTop="166dp"
        android:layout_marginEnd="176dp"
        android:layout_marginBottom="698dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Edit"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="18sp"
        android:translationX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,RtlCompat,TextContrastCheck" />

    <TextView
        android:id="@+id/Name_Section"
        android:layout_width="46dp"
        android:layout_height="27dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="203dp"
        android:layout_marginEnd="353dp"
        android:layout_marginBottom="661dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Name"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="18sp"
        android:translationX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,RtlCompat,TextContrastCheck" />

    <EditText
        android:id="@+id/edtFirstName"
        android:layout_width="383dp"
        android:layout_height="58dp"
        android:layout_marginHorizontal="32dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="603dp"
        android:background="@drawable/rectangle_corner_transparent_black"
        android:hint="First Name"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:textColor="@android:color/black"
        android:textColorHint="#A9A9A9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edtLastName"
        android:layout_width="383dp"
        android:layout_height="58dp"
        android:layout_marginHorizontal="32dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="545dp"
        android:background="@drawable/rectangle_corner_transparent_black"
        android:hint="Last Name"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:textColor="@android:color/black"
        android:textColorHint="#A9A9A9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/Hometown_Section"
        android:layout_width="86dp"
        android:layout_height="28dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="380dp"
        android:layout_marginEnd="317dp"
        android:layout_marginBottom="483dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Hometown"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="18sp"
        android:translationX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,RtlCompat,TextContrastCheck" />

    <EditText
        android:id="@+id/edtHometown"
        android:layout_width="383dp"
        android:layout_height="58dp"
        android:layout_marginHorizontal="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="408dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="425dp"
        android:background="@drawable/rectangle_corner_transparent_black"
        android:hint="City, State"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:textColor="@android:color/black"
        android:textColorHint="#A9A9A9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Bio_Section"
        android:layout_width="43dp"
        android:layout_height="25dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="496dp"
        android:layout_marginEnd="362dp"
        android:layout_marginBottom="370dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="Bio"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="18sp"
        android:translationX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,RtlCompat,TextContrastCheck" />

    <TextView
        android:id="@+id/Character_Count"
        android:layout_width="53dp"
        android:layout_height="21dp"
        android:layout_marginStart="341dp"
        android:layout_marginTop="496dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="374dp"
        android:fontFamily="@font/futura"
        android:gravity="start"
        android:text="0/150"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="18sp"
        android:translationX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,RtlCompat,TextContrastCheck" />

    <EditText
        android:id="@+id/edtBio"
        android:layout_width="382dp"
        android:layout_height="224dp"
        android:layout_marginHorizontal="32dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="521dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="146dp"
        android:background="@drawable/rectangle_corner_transparent_black"
        android:hint="150 characters"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:textColor="@android:color/black"
        android:textColorHint="#A9A9A9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>